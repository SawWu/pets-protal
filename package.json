    "buildDev": "npm run clean && cross-env NODE_ENV='dev' webpack --config webpack/webpack.dev.config.js --mode production",
    "buildUat": "npm run clean && cross-env NODE_ENV='uat' webpack --config webpack/webpack.uat.config.js --mode production",
    "_build": "npm run clean && cross-env NODE_ENV='production' webpack --config webpack/webpack.prod.config.js --mode production",
    "clean": "rimraf ./build"
  },
